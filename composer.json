{
  "name": "lochmueller/html5videoplayer-powermail",
  "type": "typo3-cms-extension",
  "description": "Connect both extenion to fetch user input (one powermail form) before the user can access the detail view.",
  "autoload": {
    "psr-4": {
      "HVP\\Html5videoplayerPowermail\\": "Classes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "HVP\\Html5videoplayerPowermail\\Tests\\": "Tests"
    }
  },
  "keywords": [
    "TYPO3 CMS",
    "HTML5 Video Player",
    "Extbase"
  ],
  "authors": [
    {
      "name": "Tim Lochm√ºller",
      "email": "webmaster@fruit-lab.de",
      "role": "Developer",
      "homepage": "https://github.com/lochmueller"
    }
  ],
  "require": {
    "php": "^7.4||^8.0",
    "typo3/cms-core": "^12.4",
    "lochmueller/html5videoplayer": "^11.0",
    "in2code/powermail": "^12.0"
  },
  "replace": {
    "typo3-ter/html5videoplayer-powermail": "self.version"
  },
  "homepage": "https://github.com/lochmueller/html5videoplayer_powermail",
  "license": "GPL-2.0-or-later",
  "support": {
    "issues": "https://github.com/lochmueller/html5videoplayer_powermail/issues"
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "html5videoplayer_powermail",
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web",
      "Package": {
        "partOfMinimalUsableSystem": true
      }
    }
  }
}
